{
  "env": {
    "commonjs": true,
    "es2022": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "class-methods-use-this": "off"
  }
}
